# ===================================
# 環境設定
# ===================================
NODE_ENV=development

# ===================================
# ポート設定
# ===================================
BACKEND_PORT=3001
FRONTEND_PORT=3000

# ===================================
# データベース設定
# ===================================
DATABASE_URL="postgresql://user:password@localhost:5432/mycats"

# ===================================
# JWT設定
# ===================================
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRES_IN=7d

# ===================================
# Clerk認証（必要な場合）
# ===================================
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# ===================================
# API設定
# ===================================
NEXT_PUBLIC_API_URL=http://localhost:3001/api/v1
